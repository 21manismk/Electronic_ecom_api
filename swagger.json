        "/consumer/signup": {
            "post": {
                "summary": "User Signup",
                "description": "User Signup",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "signup object",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "role": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string",
                                    "format": "email"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "confirm_password": {
                                    "type": "string"
                                },
                                "mobile_no": {
                                    "type": "integer",
                                    "format": "number"
                                }
                            },
                            "required": [
                                "name",
                                "email",
                                "role",
                                "password",
                                "confirm_password",
                                "mobile_no"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Operation",
                        "schema": {
                            "$ref": "#/definitions/SucessResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid status value",
                        "schema": {
                            "$ref": "#/definitions/InvalidResponse"
                        }
                    }
                }
            }
        },
